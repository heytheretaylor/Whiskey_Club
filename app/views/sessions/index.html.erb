<h1>Sessions</h1>

<% @sessions.each do |session| %>
<br>
<div class="session">
<div class="session-date"><%= session.date_of.to_s%>:</div>
<div class="session-id">session-id:<%= session.id %></div>
<% session.users.each do |user| %>
<div class="session-user">session-user:<%= user.email %></div>
<% end %>
</div>

<% end %>